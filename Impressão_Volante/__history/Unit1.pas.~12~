procedure TForm1.BitBtn1Click(Sender: TObject);
begin
  Relatorio := 'R2';
  ppReport2.Print;
end;

procedure TForm1.btnOpenClick(Sender: TObject);
begin
  if not MyConnection1.Connected then
    MyConnection1.Connect;
  MyQuery1.Open;
end;

procedure TForm1.btnPrintClick(Sender: TObject);
begin
  Relatorio := '';
  ppReport1.Print;
end;

